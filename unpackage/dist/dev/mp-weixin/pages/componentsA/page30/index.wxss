.page30-wrap {
  min-width: 100wh;
  height: 100vh;
  position: relative;
  overflow: hidden;
}
.page30-wrap .startBtn {
  width: 160px;
  height: 50px;
  position: absolute;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
  margin: auto;
  z-index: 300;
}
.page30-wrap .brid {
  width: 120px;
  height: 100px;
  background-image: url(https://datastream.affectai.cn/callmaterial/images/page23/brid.gif);
