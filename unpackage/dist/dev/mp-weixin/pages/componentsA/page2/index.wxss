.page122-wrap {
  min-height: 100vh;
  background-image: url(https://datastream.affectai.cn/callmaterial/images/page2/bg-2.png);
  background-position: center;
  background-size: 100% 100%;
  background-repeat: no-repeat;
  position: relative;
}
.page122-wrap .content {
  position: absolute;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
  margin: auto;
  width: 310px;
  height: 230px;
  text-align: center;
}

