.nextPage {
  width: 110px;
  height: 45px;
  line-height: 45px;
  text-align: center;
  border-radius: 5px;
  background-color: #6fcbeb;
  color: #000;
  font-size: 14px;
  font-weight: 400;
  margin: 50px auto;
}
.page7-wrap {
  min-height: 100vh;
  background-image: url(https://datastream.affectai.cn/callmaterial/images/page5/bg.png);
  background-position: center;
  background-size: cover;
  background-repeat: no-repeat;
  position: relative;
  overflow-y: scroll;
}
.page7-wrap .submit1 {
  width: 100px;
  height: 43px;
  line-height: 43px;
  text-align: center;
  border-radius: 5px;
  background-color: #6fcbeb;
  color: #000;
  font-size: 14px;
  font-weight: 400;
  margin: 20px auto 50px auto;
}
.page7-wrap .dialogue {
  width: 96%;
  margin: 0 auto;
  position: absolute;
  left: 0;
  right: 0;
  bottom: 5px;
  margin: auto;
}
.page7-wrap .dialogue .top {
  width: 110px;
  height: 36px;
  line-height: 36px;
  background-color: #C8F0FA;
  color: #434343;
  font-size: 18px;
  text-align: center;
  border-top-right-radius: 8px;
  border-top-left-radius: 8px;
}
.page7-wrap .dialogue .center {
  display: flex;
  background-color: #f1f1f4;
  border-top-right-radius: 10px;
  border-bottom-left-radius: 10px;
  border-bottom-right-radius: 10px;
  height: 158px;
  box-sizing: border-box;
  padding: 11px 21px;
}

